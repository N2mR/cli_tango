package cli_tango;

import java.io.File;
import java.io.FileReader;
import java.io.IOException;
public class Main {

	public static void main(String[] args) {
		
		
		try {
			File file = new File("../data.txt");
			FileReader fr = new FileReader(file);
			System.out.println(fr);
			
			fr.close();
			
		} catch(IOException e) {
			e.printStackTrace();
		}

	}

}
